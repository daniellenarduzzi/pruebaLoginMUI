{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/atoms/*": ["components/atoms/*"],
      "@/molecules/*": ["components/molecules/*"],
      "@/constants": ["utils/constants"],
      "@/themes/*": ["utils/themes/*"],
      "@/apolloUtils/*": ["utils/apollo/*"],
      "@/authUtils": ["utils/auth"],
      "@/authUtils/*": ["utils/auth/*"],
      "@/dbUtils": ["utils/db"],
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx", ".old/button/index.tsx"
  ]
}
